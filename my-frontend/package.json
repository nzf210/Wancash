  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "scr1": "~/Desktop/dev/wancash_staking/my-frontend/watch-errors.sh",
    "scr2": "~/Desktop/dev/wancash_staking/my-frontend/watch-errors-auto.sh",
    "dev": "vite",
    "dev:all": "run-p dev c:dev",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:unit": "cypress run --component",
    "test:unit:dev": "cypress open --component",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "gen-types": "bunx supabase gen types typescript --project-id lkukgmezfsezunqhrabg > src/types/supabase.d.ts",
    "c:dev": "cd cloude-flare-wancash && bun run dev",
    "c:deploy": "cd cloude-flare-wancash && bun run deploy",
    "c:logs": "cd cloude-flare-wancash && bun run logs",
    "c:test": "cd cloude-flare-wancash && bun run test:local",
    "c:env": "bun run cloude-flare-wancash/scripts/generate-env-only.ts"
  },
