{
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "git": {
    "deploymentEnabled": {
      "*": false
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api-testnet.wancash.org/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}